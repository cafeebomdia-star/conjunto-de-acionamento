{
  "headers": [
    {
      "source": "/(.*).(tsx|ts|js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:tsx|ts|js|css|png|jpg|svg|json|html|ico)$).*|)",
      "destination": "/index.html"
    }
  ]
}